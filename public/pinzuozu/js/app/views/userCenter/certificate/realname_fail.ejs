<div class="page-list-bg p10 mt20 frame-border">

  <div class="mt20 ml20 crred">
    <h3><i class="fa  fa-times-circle"></i>审核未通过</h3>
  </div>

  <div class="user-cr mt10">
    <form class="form-horizontal  mt30" role="form">
      <div class="form-group module-success-form">
        <label class="col-xs-3 control-label f14 cr666">
          姓名：
        </label>
        <div class="col-xs-3">
          <p class="form-control-static">
            <%=identity.user_realname %>
          </p>
        </div>
      </div>

      <div class="form-group module-success-form">
        <label class="col-xs-3 control-label f14 cr666">
          性别：
        </label>
        <div class="col-xs-3">
          <p class="form-control-static">
            <%= identity.user_gender=='0'?'男':'女' %>
          </p>
        </div>
      </div>

      <div class="form-group module-success-form">
        <label class="col-xs-3 control-label f14 cr666">
          身份证：
        </label>
        <div class="col-xs-3">
          <p class="form-control-static">
            <%=identity.identity_card_no %>
          </p>
        </div>
      </div>
      
      <div class="form-group module-success-form">
        <label class="col-xs-3 control-label f14 cr666">
          身份证照：
        </label>
        <div class="col-xs-3 module-success-cardimg">
          <img src="http://192.168.1.115:3000/<%=identity.identity_image %>" />
        </div> 
      </div>
      
      <div class="form-group module-success-form">
        <label class="col-xs-3 control-label f14 cr666">
          审核时间：
        </label>
        <div class="col-xs-3 ">
          <p class="form-control-static">
            <%=identity.updated_at %>
          </p>
        </div>
      </div>

      <div class="form-group module-success-form">
        <label class="col-xs-3 control-label f14 cr666">
          未通过原因：
        </label>
        <div class="col-xs-3 ">
          <p class="form-control-static">
            <%=identity.identity_remark==null?"无":identity.identity_remark %>
          </p>
        </div>
      </div>

      <div class="form-group module-success-form">
        <div class="col-xs-offset-3 col-xs-6">
          <button type="button" id="identity_realname_again" class="btn btn-primary frame-nradius">
            重新认证
          </button>
        </div>
      </div>

    </form>
  </div>
</div>