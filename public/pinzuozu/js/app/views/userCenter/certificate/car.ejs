<div class="page-list-bg p10 mt20 frame-border">

  <div class="mt20 ml20">
    <h4>我的车辆</h4>
  </div>
  <div class="user-cr">
    <form class="form-horizontal mt50 " role="form">

      <div class="form-group m30">
        <label for="inputMobile" class="col-xs-3 control-label cr666 f14">车牌号码：</label>
        <div class="col-xs-2">
          <select class="form-control frame-nradius" id='city-plate'>
            <option value='京'>京</option>
            <option value='冀'>冀</option>
            <option value='苏'>苏</option>
          </select>
        </div>
        <div class="col-xs-4">
          <input type="tel" class="form-control frame-nradius" id="car_plate_no" name="car_plate_no" placeholder="车牌号"></div>
        <span id="car_plate_no-msg" class="col-xs-3 f12 crred control-label tl"> 
          
        </span>
      </div>
      <div class="form-group form-group-sm ">
        <label for="nickName" class="col-xs-3 cr666 f14 control-label">座位数：</label>
        <div class="col-xs-2">
          <select class="form-control" name="car_seats">
            <option value='1'>1</option>
            <option value='2'>2</option>
            <option value='3'>3</option>
            <option value='4'>4</option>
          </select>
        </div>
        <div class="col-xs-2">
          <span class="cr14">(含驾驶员)</span>
        </div>
      </div>
      <div class="form-group m30">
        <label for="inputMobile" class="col-xs-3 control-label cr666 f14">发动机编号：</label>
        <div class="col-xs-6">
          <input type="tel" class="form-control frame-nradius" id="car_engine_no" name="car_engine_no " placeholder="发动机编号"></div>
        <span id="car_engine_no" class="col-xs-3 f12 crred control-label tl">
          
        </span>
      </div>
      <div class="form-group form-group-sm ">
        <label for="nickName" class="col-xs-3 control-label cr666 f14">车种：</label>
        <div class="col-xs-3">
          <select class="form-control" name="car_type">
            <option value="城市越野车">城市越野车</option>
            <option value="跑车">跑车</option>
            <option value="多功能用途车">多功能用途车</option>
            <option value="迷你轿车">迷你轿车</option>
            <option value="小型轿车">小型轿车</option>
            <option value="中级轿车">中级轿车</option>
            <option value="小型客车">小型客车</option>
            <option value="豪华轿车">豪华轿车</option>
          </select>
        </div>
      </div>
      <div class="form-group form-group-sm ">
        <label for="nickName" class="col-xs-3 control-label cr666 f14">车型：</label>
        <div class="col-xs-3">
          <select class="form-control">
            <option value="宝马">宝马</option>
            <option value="奔驰">奔驰</option>
            <option value="奥迪">奥迪</option>
          </select>
        </div>
      </div>
      <div class="form-group m30">
        <label for="inputMobile" class="col-xs-3 control-label cr666 f14">保险公司名称：</label>
        <div class="col-xs-6">
          <input type="tel" class="form-control frame-nradius" id="insurance_corporate" name="insurance_corporate " placeholder="保险公司名称"></div>
        <span id="insurance_corporate-msg" class="col-xs-3 f12 crred control-label tl">
          
        </span>
      </div>
      <div class="form-group form-group-xs ">
        <label for="nickName" class="col-xs-3 control-label f14 cr666">保险到期时间：</label>
        <div class="control-group col-xs-3">
          <div class="controls">
            <div class="input-prepend input-group ">
              <span class="add-on input-group-addon border-radius-style">
                <i class="glyphicon glyphicon-calendar fa fa-calendar"></i>
              </span>
              <input type="text" id='insurance_expired' class="form-control border-radius-style offer-date-input" name="insurance_expired" />
            </div>
          </div>
        </div>
        <span id="mobile-verify" class="col-xs-3 f12 crred control-label tl">
          
        </span>
      </div>
      <div class="form-group m30">
        <label for="inputMobile" class="col-xs-3 control-label cr666 f14">保险单号：</label>
        <div class="col-xs-6">
          <input type="tel" class="form-control frame-nradius" id="insurance_no" name="insurance_no " placeholder="保险单号"></div>
        <span id="mobile-verify" class="col-xs-3 f12 crred control-label tl">
          <i class="fa fa-times-circle crred fa-lg mr5"></i>
          输入保险单号
        </span>
      </div>

      <div class="form-group form-group-sm ">
        <label for="nickName" class="col-xs-3 control-label cr666 f14">保险单扫描件：</label>
        <div class="col-xs-3">
          <input type="file" id="insurance_image" name="insurance_image" />
          <img id="insurance-image" src="" style="display:none;" />
        </div>
      </div>
      <div class="form-group">
        <div class="col-xs-offset-3">
          <div class="col-xs-4">
            <a class="com-rposition"  data-toggle="modal" data-target=".bs-example-modal-sm" >
              <i  class="fa fa-search module-identif-i"></i>
            </a>
            <img class="module-identif-driversimg" src="images/driver1.jpg" />
          </div>
        </div>
      </div>
      <div class="modal fade bs-example-modal-sm " id="mymodal">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal">
                <span aria-hidden="true">&times;</span>
                <span class="sr-only">Close</span>
